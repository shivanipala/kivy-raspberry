<TestResultScreen>:

    FloatLayout:

        Label:
            markup: True
            text: "[color=#0000ff][font=OpenSans]Test Results[/color][/font]"
            size_hint: (None,None)
            size: (50,10)
            pos: (385,405)
            font_size: '23px'

        Button:
            background_color: [0,0,200,0.8]
            markup: True
            text: "[color=#ffffff][b]Generate Report[/b][/color]"
	        size_hint: (None,None)
	        height: 38
            width: 130
            pos: (40,20)
            on_press: root.mail_popup()
        Button:
            background_color: [0,0,200,0.8]
		    markup: True
		    text: "[color=#ffffff][b]Proceed For Diagnosis[/b][/color]"
		    size_hint: (None,None)
		    height: 38
            width: 180
            pos: (600,20)
        Button:
            background_color: [0,0,200,0.8]
            markup: True
            text: "[color=#ffffff][b]   Pre\nResults[/b][/color]"
	        size_hint: (None,None)
	        height: 50
            width: 70
            pos: (40,290)
            on_press: sm2.current= 'Pre_Result'
        Button:
            background_color: [0,0,200,0.8]
            markup: True
            text: "[color=#ffffff][b]  Post\nResults[/b][/color]"
	        size_hint: (None,None)
	        height: 50
            width: 70
            pos: (40,230)
            on_press: sm2.current= 'Post_Result'



    ScreenManager:
        id: sm2
        Pre_Result:
        Post_Result:

